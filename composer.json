{
    "name": "acemodules/ace-datagrid",
    "description": "A ZF2 module for displaying, sorting, and paginating doctrine entities.",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "^2.5",
        "doctrine/doctrine-orm-module": "^0.9.2",
        "neilime/zf2-twb-bundle": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Ace\\Datagrid\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "component": "Ace\\Datagrid",
            "config-provider": "Ace\\Datagrid\\ConfigProvider"
        }
    }
}
